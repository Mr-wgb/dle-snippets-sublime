  <content><![CDATA[
this.$http.post(url, data, options)
.then(({data,ok,statusText}) => {
  if (ok && !data.status) {
    // successful
    console.log(data)
  }else{
    // warning
    this.$message({
      message: data.msg,
      type: 'warning'
    });
  }
}).catch(err => {
  // error
  this.$message.error('哦，出现了一个错误，请联系管理员');
  console.error(err)
});
]]></content>
