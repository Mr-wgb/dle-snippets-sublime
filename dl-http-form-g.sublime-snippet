  <content><![CDATA[
Vue.http.post(url, data, {
  emulateJSON:true
})
.then(({data,ok,statusText}) => {
  if (ok && !data.status) {
    // successful
    console.log(data)
  }else{
    // warning
    Vue.\$message({
      message: data.msg,
      type: 'warning'
    });
  }
}).catch(err => {
  // error
  Vue.\$message.error('哦，出现了一个错误，请联系管理员');
  console.error(err)
});
]]></content>
  <tabTrigger>http:form-g</tabTrigger>
  <discription>Vue.http.post()</discription>
  <scope>source.js</scope>
</snippet>
